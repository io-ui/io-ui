{
    "name": "io-ui",
    "version": "0.0.1",
    "description": "io vue ui",
    "license": "MIT",
    "main": "package/io-ui.js",
    "keywords": [
        "vue",
        "io-ui"
    ],
    "scripts": {
        "dev": "webpack-dev-server --progress --history-api-fallback --config build/webpack.config.dev.js",
        "start": "npm run dev",
        "lint": "eslint ./",
        "prod:win": "npm run lint && rd /s/q package && webpack --config build/webpack.config.prod.js",
        "prod": "npm run lint && rm -rf package && webpack --config build/webpack.config.prod.js",
        "karma-start": "npm run lint && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
        "karma-single": "npm run lint && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "coverall": "cat test/unit/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/io-ui/io-ui.git"
    },
    "bugs": {
        "url": "https://github.com/io-ui/io-ui/issues"
    },
    "homepage": "https://github.com/io-ui/io-ui#readme",
    "dependencies": {
        "@vue/composition-api": "^0.6.4",
        "vue": "^2.5.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "@vue/test-utils": "^1.1.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.6",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "chai": "^4.2.0",
        "chai-spies": "^1.0.0",
        "copy-webpack-plugin": "^5.0.4",
        "coveralls": "^3.1.0",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "eslint": "^7.19.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-vue": "^7.5.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.4",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "html-webpack-plugin": "^3.2.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "karma": "^5.2.3",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-mocha": "^2.0.1",
        "karma-sinon-chai": "^2.0.2",
        "karma-sourcemap-loader": "^0.3.8",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "^4.0.2",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.3",
        "mini-css-extract-plugin": "^0.11.0",
        "mocha": "^8.1.3",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.2.0",
        "sinon": "^9.0.3",
        "sinon-chai": "^3.5.0",
        "url-loader": "^2.1.0",
        "vue-loader": "15.7.0",
        "vue-router": "^3.1.3",
        "vue-template-compiler": "^2.4.4",
        "webpack": "4.39.2",
        "webpack-cli": "3.3.6",
        "webpack-dev-server": "3.8.0",
        "webpack-merge": "4.2.1"
    },
    "files": [
        "package"
    ],
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    }
}
